<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1" />
    <httpRuntime targetFramework="4.8.1" />
  </system.web>
	<appSettings>
		<add key="db.provider" value="MySql.Data.MySqlClient" />
		<add key="db.hostname" value="[Complete con su hostname]" />
		<add key="db.port" value="[Complete con su port]" />
		<add key="db.username" value="[Complete con su username]" />
		<add key="db.password" value="[Complete con su password]" />
		<add key="db.database" value="[Complete con su database]" />	
	</appSettings>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
		<dependentAssembly>
			<assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
			<bindingRedirect oldVersion="0.0.0.0-4.2.4.0" newVersion="4.2.4.0" />
		</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>